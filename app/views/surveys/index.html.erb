<p id="notice"><%= notice %></p>
<%= link_to 'student', students_path %>
<%= link_to 'surveys', surveys_path %>
<%= link_to 'operators', operators_path %>

<h1>Listing Surveys</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Ssn</th>
      <th>Email</th>
      <th>Concentration</th>
      <th>Dob</th>
      <th>Gender</th>
      <th>Ethnicity</th>
      <th>Enrolled</th>
      <th>Previouscollege</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @surveys.each do |survey| %>
      <tr>
        <td><%= survey.Firstname %></td>
        <td><%= survey.Lastname %></td>
        <td><%= survey.SSN %></td>
        <td><%= survey.email %></td>
        <td><%= survey.Concentration %></td>
        <td><%= survey.DoB %></td>
        <td><%= survey.Gender %></td>
        <td><%= survey.Ethnicity %></td>
        <td><%= survey.Enrolled %></td>
        <td><%= survey.previouscollege %></td>
        <td><%= link_to 'Show', survey %></td>
        <td><%= link_to 'Edit', edit_survey_path(survey) %></td>
        <td><%= link_to 'Destroy', survey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Survey', new_survey_path %>
