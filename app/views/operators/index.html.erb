<p id="notice"><%= notice %></p>
<%= link_to 'student', students_path %>
<%= link_to 'surveys', surveys_path %>
<%= link_to 'operators', operators_path %>

<h1>Listing Operators</h1>

<table>
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Concentration</th>
      <th>Projectyear</th>
      <th>Email</th>
      <th>Gpa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @operators.each do |operator| %>
      <tr>
        <td><%= operator.Firstname %></td>
        <td><%= operator.Lastname %></td>
        <td><%= operator.Concentration %></td>
        <td><%= operator.projectyear %></td>
        <td><%= operator.email %></td>
        <td><%= operator.GPA %></td>
        <td><%= link_to 'Show', operator %></td>
        <td><%= link_to 'Edit', edit_operator_path(operator) %></td>
        <td><%= link_to 'Destroy', operator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Operator', new_operator_path %>
